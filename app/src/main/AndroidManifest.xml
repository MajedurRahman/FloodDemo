<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mmr.meza.flooddemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Activity.MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Activity.HomeActivity"
            android:screenOrientation="portrait" />
        <activity android:name=".Activity.PointChart" />
        <activity android:name=".Activity.BarChartAcivity" />
        <activity android:name=".Activity.LineChartActivity" />
        <activity android:name=".Activity.SpeedChartActivity" />
        <activity android:name=".Activity.PieChartActivity" />
        <activity android:name=".Activity.ColumnChartActivity" />
        <activity android:name=".Activity.BubbleChartActivity" />
        <activity android:name=".Activity.ComboLineColumnChartActivity" />
        <activity android:name=".Activity.LineColumnDependencyActivity" />
        <activity android:name=".Activity.PreviewColumnChartActivity" />
        <activity android:name=".Activity.ViewPagerChartsActivity" />
        <activity
            android:name=".Activity.AppHome"

            android:theme="@style/AppTheme"></activity>



    </application>

</manifest>